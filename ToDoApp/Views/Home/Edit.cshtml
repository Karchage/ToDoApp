@model ToDoApp.Entity.ToDo
@{
    ViewBag.Title = "Edit";
}
<form asp-action="Edit" asp-controller="Home" asp-route-id="@Model.Id">
    <div class="form-group">
        <label asp-for="Context" class="control-label">Context</label>
        <input type="text" asp-for="Context" class="form-control" />

    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-default" data="@Model" />
    </div>
</form>